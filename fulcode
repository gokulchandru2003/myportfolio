    			//JAVASCRIPT
    <script>
    $(document).ready(function() {
    $('#printButton').on('click', function() {
      window.print();
    });
  });

// smooth scroll
$(document).ready(function(){
    $(".navbar .nav-link").on('click', function(event) {

        if (this.hash !== "") {

            event.preventDefault();

            var hash = this.hash;

            $('html, body').animate({
                scrollTop: $(hash).offset().top
            }, 700, function(){
                window.location.hash = hash;
            });
        } 
    });
});

</script>

